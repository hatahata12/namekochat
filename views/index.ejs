        <!-- Add your site or application content here -->
        <div id="wrapper">
        	<header>
            	<h1><p>なめこ</p></h1>
            </header>
            <div id="main_content">
            <%
            var flg = false;
            for (var i = 0, max = viewObj.length; i < max; i++) {
            if (viewObj[i].ip) {
            flg = true;
            %>
            <div class="user" data-id="<%= viewObj[i].id %>" data-ip="<%= viewObj[i].ip %>"><h2><p>なめこ</p></h2><p></p></div>
            <%
            } else {
            %>
            <div class="user" data-id="<%= viewObj[i].id %>"><h2><p>なめこ</p></h2><p></p></div>
            <%
            }
            }
            %>
            </div>
        </div>
        <div id="footer_wrapper">
        	<footer>
            <div id="in">
                    <%
                    if (flg) {
                    %>
                    <input style="display : none" type="submit" id="send_in" value="んふんふする" />
                    <div id="send" style="display : block">
                    <input type="text" name="message" id="msg" value="" />
                    <input type="submit" id="send_msg" value="しゃべる" />
                    <p><input type="submit" id="logout" value="んふんふやめる" /></p>
                    <%
                    } else {
                    %>
                    <input type="submit" id="send_in" value="んふんふする" />
					<div id="send">
						<input type="text" name="message" id="msg" value="" />
                        <input type="submit" id="send_msg" value="しゃべる" />
                        <p><input type="submit" id="logout" value="んふんふやめる" /></p>
                   <% } %>
					</div>
                </div>
            </footer>
        </div>

        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.0/jquery.min.js"></script>
        <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.9.0.min.js"><\/script>')</script>
		<script type="text/javascript" src="/socket.io/socket.io.js"></script>
		<script src="js/vendor/jquery.ui.core.js"></script>
		<script src="js/vendor/jquery.ui.widget.js"></script>
		<script src="js/vendor/jquery.ui.mouse.js"></script>
        <script src="js/vendor/jquery.ui.draggable.js"></script>
        <script src="js/vendor/jquery.ui.touch-punch.js"></script>
        <script src="js/socket.js"></script>
		<script src="js/main.js"></script>
